var startcolor= new Array(255,255,255); // start color (red, green, blue)
var endcolor=new Array(0,0,0); // end color (red, green, blue)

var fcontent=new Array();
begintag='<div style="font: normal 10px verdana; padding: 5,5,5,5;">'; //set opening tag, such as font declarations
fcontent[0]="<b>Students:</b> Find out more about our program and <a href='program.php'>how to apply</a>.";
